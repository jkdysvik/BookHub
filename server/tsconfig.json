{
    "compilerOptions": {
      "target": "ESNext",
  
      "module": "commonjs",
      "lib": ["dom", "es6"],
  
      "outDir": "./dist",
      "removeComments": true,
  
      "esModuleInterop": true,
  
      "forceConsistentCasingInFileNames": true,
  
      "strict": true,
  
      "skipLibCheck": true
    }
  }
  